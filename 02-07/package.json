    "start": "nodemon",
    "db-migrate": "cd db-migration && sequelize-cli db:migrate",
    "db-migrate:undo": "cd db-migration && sequelize-cli db:migrate:undo:all",
    "db-migrate:reset": "npm run db-migrate:undo && npm run db-migrate",
    "db-seed": "cd db-migration && sequelize-cli db:seed:all",
    "test": "jest --testPathPattern=src/tests/unit",
    "migrate-test-db": "cross-env NODE_ENV=test npm run db-migrate:reset",
    "test:api": "npm run migrate-test-db && cross-env NODE_ENV=test jest --testPathPattern=src/tests/integration --testTimeout=15000 --forceExit --detectOpenHandles"
  },
